#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules/hardhat/internal/cli/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules/hardhat/internal/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules/hardhat/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules/hardhat/internal/cli/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules/hardhat/internal/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules/hardhat/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/hardhat@2.22.5_ts-node@10.9.2_@types+node@20.14.6_typescript@5.4.5__typescript@5.4.5/node_modules:/Users/glitch/Desktop/Oasis_shunk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
fi
